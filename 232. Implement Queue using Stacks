class MyQueue {
    Stack <Integer> st;
    Stack <Integer> rt;

    public MyQueue() {
        st = new Stack();
        rt = new Stack();
    }
    
    public void push(int x) {
        st.push(x);
    }
    
    public int pop() {
        peek();
        return rt.pop();
    }
    
    public int peek() {
        if(rt.empty()){
            while(! st.empty()){
                rt.push(st.pop());
            }
        }
        return rt.peek();
    }
    
    public boolean empty() {
        return st.empty() && rt.empty();
    }
}

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue obj = new MyQueue();
 * obj.push(x);
 * int param_2 = obj.pop();
 * int param_3 = obj.peek();
 * boolean param_4 = obj.empty();
 */
